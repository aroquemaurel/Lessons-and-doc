fonction nbEquipesAuDessusMoyenne(entree matchs <TabMatchs>, entree moyenne <Reel>)
retourne <Entier>
glossaire
	e <Entier>;
	cumulE <Entier>;
debut
	cumulE <- 0;
	e <- 0;
	tantque e <= NB_EQUIPES faire
		si matchs[e].gagnes > moyenne alors
			cumulE <- cumulE + 1;	
		fin si;
	fin tantque;
	retourner(cumulE);
fin


