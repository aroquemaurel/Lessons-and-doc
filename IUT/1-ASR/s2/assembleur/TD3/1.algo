glossaire
	R0 $\equiv$ i
	R1: .pTab ou .ptrouve ou .pRang
	R2: .pN puis n
	R3: .pEle puis ele
	R4: tab[i]
debut
	R0 <- #0;
	tantque (R2 <- .pN, R2 <- R2$\uparrow$, R0 < R2) et
			(R1 <- .pTab, R4 <- (R1+DLG(2,R0)), R3 <- .pEle, 
			 R3 <-R3$\uparrow$, R4 /= R3) faire
		R0 <- R0 + #1;
	fin tantque;
	si R0 < R2 alors
		R1 <- .pRang;
		R1 $\uparrow$ <- R0;
		R1 <- .pTrouve;
		R1$\uparrow$.B <- #VRAI;
	sinon	
		R1 <- .pTrouve;
		R1$\uparrow.B <- #FAUX;
	fin si;
fin
		
	
