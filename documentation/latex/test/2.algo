-- + propriétés en commentaire

procedure créerensembleVide(sortie ens <Ensemble[T]>);

fonction estVide(entree ens <Ensemble[T]>) retourne <Booleen>;

fonction appartient(entree ens <Ensemble[T]>, entree e <T>) 
	retourne <Booleen>;

procedure ajouter(maj ens <Ensemble[T]>, entree e <T>)
	declenche elementExistant, ensemblePlein;

procedure supprimer(maj ens <Ensemble[T], entree e <T>);
	declenche elementInexistant, ensembleVide;

fonction cardinal(entree ens <Ensemble[T]>)
	retourne <Entier>;

