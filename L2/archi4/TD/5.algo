N <- 50;
t[N];
i <- 0;
size <- N;
tmp;

tantque size > 1 faire
	i <- 0;
	tantque i < (size-1) faire
		tmp <- t[i];
		t[i] <- t[i+1];
		t[i+1] <- tmp;
		i <- i + 1;
	faire
	size <- size - 1;
fin tantque;













