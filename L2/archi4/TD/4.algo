r0 <- @t1
r1 <- @t2

si r0 < r1 alors
	r2 <- N;
	tantque r2 != 0 faire
		r2 = r2 - 1;
		r0[r2] <- r1[r2];
	fin tantque;
sinon
	tantque r2 != N faire
		r2 <- 0;
		r0[r2] <- r1[r2];
		r2 = r2 + 1;
	fin tantque;
fin si;
	








-- Autre solution
si t1 < t2 alors
	i <- N-1;
	f <- -1;
	p <- -1;
sinon
	i <- 0;
	f <- N;
	p <- 1;
fin si;
tantque i != f faire
	t2[i] <- t1[i];
	i <- i + p;
fin tantque;

