			\item[2 $$(x \in E) \wedge (x \in F)$$
			\item[3] $$(x \in E) \wedge (x \not\in F)$$
			\item[]  $$x \in (C(F)) \wedge(X \in E) = \neg(X \in P) \wedge (x \in E)$$
			\item[J]  $$(E \cap FÃ ) C C(C(E) \cup C(F))$$		
			\item[]  $$x \in(E \cup F)$$
			\item[5]  
				\begin{eqnarray*}
					x &\in& C(E \cup C(F))\\
					\neg X &\in&(E\cup C(F))\\
					\not(X \in E &\wedge& X \in C(F))\\
					\neg(x \in E &\wedge& \neg (X \in F))\\
				\end{eqnarray*}
		\end{description}
		
			

	\subsection{Expression ensembliste}
	$$X \in (A \cup B)$$
	\subsubsection{Compl.}
	\begin{eqnarray*}
		C(C(A)) &=&A\\
		C(a\cup B) &=& C(A) \cap C(B)\\
		C(A \cap B) &=& C(A) \cup C(B)\\
	\end{eqnarray*}
